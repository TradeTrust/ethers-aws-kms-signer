{
  "name": "@tradetrust-tt/ethers-aws-kms-signer",
  "version": "2.1.3",
  "description": "An Ethers.js compatible signer that connects to AWS KMS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeTrust/ethers-aws-kms-signer.git"
  },
  "bugs": {
    "url": "https://github.com/TradeTrust/ethers-aws-kms-signer/issues"
  },
  "homepage": "https://github.com/TradeTrust/ethers-aws-kms-signer#readme",
  "main": "dist/index.js",
  "types": "dist/ts/src/index.d.ts",
  "author": {
    "name": "TradeTrust",
    "url": "https://github.com/TradeTrust"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "open-cli coverage/lcov-report/index.html",
    "lint": "eslint . --ext js,ts,tsx",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --emitDeclarationOnly && babel src -d dist --ignore src/**/*.spec.ts,src/**/*.test.ts -x .js,.ts,.tsx",
    "preversion": "npm run lint && npm test && npm run build",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --ext js,ts,tsx",
      "git add"
    ]
  },
  "keywords": [
    "tradetrust"
  ],
  "dependencies": {
    "@aws-sdk/client-kms": "^3.830.0",
    "@ethersproject/abstract-provider": "^5.8.0",
    "@ethersproject/abstract-signer": "^5.8.0",
    "@ethersproject/bytes": "^5.8.0",
    "@ethersproject/hash": "^5.8.0",
    "@ethersproject/keccak256": "^5.8.0",
    "@ethersproject/properties": "^5.8.0",
    "@ethersproject/transactions": "^5.8.0",
    "@types/node": "^18.19.112",
    "asn1.js": "^5.4.1",
    "bn.js": "^5.2.2",
    "debug": "^4.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@commitlint/prompt": "^17.8.1",
    "@octokit/core": "^5.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.3",
    "@types/asn1": "^0.2.4",
    "@types/debug": "^4.1.12",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "commitizen": "^4.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^13.3.0",
    "open-cli": "^7.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^4.4.1",
    "semantic-release": "^22.0.12",
    "semantic-release-npm-github-publish": "^1.5.5",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
